/* pages/gen/gen.wxss */
page {
  width: 100%;
}

.modalMask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal {
  overflow: hidden;
  position: absolute;
  left: 0;
  z-index: 9500;
  width: 90%;
  margin: 20% 5%;
}

.imgContainer {
  width: 100%;
  height: 1000rpx;
  margin: 0 auto;
}

.container {
  padding: 0;
  justify-content: unset;
}

.bg {
  position: absolute;
  z-index: -1;
}

.canvasView {
  width: 580rpx;
  height: 580rpx;
  text-align: center;
  margin-top: 20%;
  background-color: rgba(255, 255, 255, 0.95);
  border: solid 2px #ba995f;
}

.starts {
  width: 576rpx;
  height: 280rpx;
  margin-top: 50rpx;
  margin-bottom: 65%;
}

.itemListContainer {
  display: flex;
  margin: 0 auto;
  width: 100%;
  height: 200rpx;
  margin-top: 50rpx;
  align-items: center;
  background-color: rgba(255, 255, 255, 0.7);
  border: solid 2px #ba995f;
}

.itemList {
  display: flex;
  justify-content: space-between;
  height: 100%;
  align-items: center;
  /* flex-wrap: wrap; */
}

.itemList .item {
  width: 120rpx;
  flex-direction: column;
  align-items: center;
  height: 120rpx;
  padding-right: 40rpx;
  padding-left: 20rpx;
}

.itemList image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 5px;
  margin: 0 auto;
  display: block;
  border: 1px solid rgb(235, 235, 245);
}

.create {
  width: 90%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
}